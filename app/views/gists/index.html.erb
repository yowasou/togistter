<p id="notice"><%= notice %></p>

<h1>Listing Gists</h1>

<table>
  <thead>
    <tr>
      <th>Gisturl</th>
      <th>Description</th>
      <th>Matome</th>
      <th>User</th>
      <th>Code</th>
      <th>Codetype</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gists.each do |gist| %>
      <tr>
        <td><%= gist.gisturl %></td>
        <td><%= gist.description %></td>
        <td><%= gist.matome_id %></td>
        <td><%= gist.user_id %></td>
        <td><%= gist.code %></td>
        <td><%= gist.codetype_id %></td>
        <td><%= link_to 'Show', gist %></td>
        <td><%= link_to 'Edit', edit_gist_path(gist) %></td>
        <td><%= link_to 'Destroy', gist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gist', new_gist_path %>
